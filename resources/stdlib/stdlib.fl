<?xml version="1.0"?>
<!DOCTYPE fbml SYSTEM "../doctypes/fbml-noext.dtd">
<fbml version="0.0">
   <method id="add-integer">
      <extend name="MethodName">Add</extend>
      <source id="a" slot="0"/>
      <source id="b" slot="1"/>
      <sink   id="c" slot="0"/>
   </method>

   <method id="print-integer">
      <extend name="MethodName">Print</extend>
      <source id="io_in" slot="0"/>
      <source id="printable" slot="1"/>
      <sink   id="io_out" slot="0"/>
   </method>
</fbml>
