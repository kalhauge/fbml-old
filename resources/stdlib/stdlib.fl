<?xml version="1.0"?>
<!DOCTYPE fbml SYSTEM "../doctypes/fbml-noext.dtd">
<fbml version="0.0">
    <method id="add-integer">
        <require name="method_name">Add</require>
        <require name="sources">  
            <source id="a" slot="0"/>
            <source id="b" slot="1"/>
        </require>
        <require name="type">
            <type slot="0">Integer</type>
            <type slot="1">Integer</type>
        </require>
        <ensure name="sinks">  
            <sink id="c" slot="0"/>
        </ensure>
        <ensure name="type">
            <type slot="0">Integer</type>
        </ensure>
        <ensure name="llvm">add</ensure>
    </method>

    <method id="add-real">
        <require name="method_name">Add</require>
        <require name="sources">  
            <source id="a" slot="0"/>
            <source id="b" slot="1"/>
        </require>
        <require name="type">
            <type slot="0">Real</type>
            <type slot="1">Real</type>
        </require>
        <ensure name="sinks">  
            <sink id="c" slot="0"/>
        </ensure>
        <ensure name="type">
            <type slot="0">Real</type>
        </ensure>
        <ensure name="llvm">fadd</ensure>
    </method>

    <method id="sub-integer">
        <require name="method_name">Substract</require>
        <require name="sources">  
            <source id="a" slot="0"/>
            <source id="b" slot="1"/>
        </require>
        <require name="type">
            <type slot="0">Integer</type>
            <type slot="1">Integer</type>
        </require>
        <ensure name="sinks">  
            <sink id="c" slot="0"/>
        </ensure>
        <ensure name="type">
            <type slot="0">Integer</type>
        </ensure>
        <ensure name="llvm">sub</ensure>
    </method>
    
